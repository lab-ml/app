{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/node": "latest",
    "sass": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@sentry/react": "^5.22.3",
    "@sentry/tracing": "^5.22.3",
    "@types/d3": "^5.7.2",
    "@types/mixpanel-browser": "^2.35.4",
    "d3": "^5.16.0",
    "mixpanel-browser": "^2.39.0"
  },
  "scripts": {
    "build": "$npm_execpath run build:ui && $npm_execpath run build:sass",
    "watch": "$npm_execpath run watch:ui & $npm_execpath run watch:sass",
    "build:ui": "tsc -p tsconfig.json",
    "watch:ui": "tsc -w -p tsconfig.json",
    "build:sass": "sass ui/src/style.scss:static/ui/css/style.css",
    "watch:sass": "sass --watch ui/src/style.scss:static/ui/css/style.css",
    "clean": "rm -rf static/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
